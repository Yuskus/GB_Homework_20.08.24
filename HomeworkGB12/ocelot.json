{
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:7038/"
  },
  "Routes": [
    {
      "UpstreamPathTemplate": "/gateway/api_storage/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Patch", "Delete" ],
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7023
        }
      ],
      "SwaggerKey": "storage"
    },
    {
      "UpstreamPathTemplate": "/gateway/api_employees/{everything}",
      "UpstreamHttpMethod": [ ],
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7209
        }
      ],
      "SwaggerKey": "employees"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "storage",
      "TransformByOcelotConfig": false,
      "Config": [
        {
          "Name": "Storage API",
          "Version": "1v",
          "Url": "https://localhost:7023/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "employees",
      "TransformByOcelotConfig": false,
      "Config": [
        {
          "Name": "Employees API",
          "Version": "1v",
          "Url": "https://localhost:7209/swagger/v1/swagger.json"
        }
      ]
    }
  ]
}